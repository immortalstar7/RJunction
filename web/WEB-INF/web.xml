<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>user_login</servlet-name>
        <servlet-class>login.user_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rest_login</servlet-name>
        <servlet-class>login.rest_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rest_branch</servlet-name>
        <servlet-class>restaurant.rest_branch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upload_menu</servlet-name>
        <servlet-class>restaurant.uploadmenu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rest_menuadd_code</servlet-name>
        <servlet-class>rest_menuadd_code</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>user_login</servlet-name>
        <url-pattern>/user_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rest_login</servlet-name>
        <url-pattern>/rest_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rest_branch</servlet-name>
        <url-pattern>/rest_branch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload_menu</servlet-name>
        <url-pattern>/upload_menu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rest_menuadd_code</servlet-name>
        <url-pattern>/rest_menuadd_code</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
